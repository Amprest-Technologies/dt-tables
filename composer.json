{
	"name": "amprest/laravel-dt",
	"type": "library",
	"description": "This is a package that provides a customizable datatables component for the Laravel Framework.",
	"homepage": "https://amprest.co.ke/",
	"keywords": [ "laravel", "datatables" ],
	"license": "MIT",
	"minimum-stability": "dev",
    "prefer-stable": true,
	"authors": [
		{
			"name": "Alvin Kaburu",
			"email": "geekaburu@amprest.co.ke"
		}
	],
	"require": {
		"php": "^8.0",
		"laravel/framework": "^11.0|^12.0"
	},
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
		"laravel/pint": "^1.2"
	},
	"autoload": {
		"psr-4": {
			"Amprest\\LaravelDT\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Amprest\\LaravelDT\\DTServiceProvider"
			]
		}
	}
}
